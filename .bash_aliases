# easier to move up
alias ..='cd ..'

# easier to go home
alias ~='cd ~'

# for opening intellij on macbook
alias intellij='open -a IntelliJ\ IDEA'

# Extra parts shown in default ls
#alias ls='ls -GFh'

# For getting graphical local weather
alias weather='curl wttr.in/78240'

# Defaults from Ubuntu, notate later
alias ll='ls -alF'
alias l='ls -CF'

# Easily show hidden files
alias la='ls -A'


